{
    "name": "abovesky/url-auth",
    "description": "Generate a url with an expiration date and signature to prevent unauthorized access",
    "keywords": [
        "security",
        "encryption",
        "sign",
        "url"
    ],
    "homepage": "https://github.com/abovesky/url-auth",
    "license": "MIT",
    "authors": [
        {
            "name": "abovesky",
            "email": "abovesky@qq.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "league/url": "^3.3",
        "indigophp/hash-compat": "^1.1"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.2",
        "henrikbjorn/phpspec-code-coverage": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "abovesky\\UrlAuth\\": "src"
        }
    },
    "scripts": {
        "test": "phpspec run"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
